{
  ".source.stan": {
    "editor": {
      "softTabs": true,
      "tabLength": 2,
      "commentStart": "// ",
      "foldEndPattern": "^\\s*\\}",
      "increaseIndentPattern": "(?x) \\{ [^}\"']* $ | \\[ [^\\]\"']* $ | \\( [^)\"']* $",
      "decreaseIndentPattern": "(?x) ^ \\s* (\\s* /[*] .* [*]/ \\s*)* [}\\])]"
    }
  }
}